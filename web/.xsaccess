{
	"exposed": true,
	"authentication": [{
		"method": "Form"
    }],

	"mime_mapping": [{
		"extension": "jpg",
		"mimetype": "image/jpeg"
    }],
	"force_ssl": false,
	"enable_etags": true,
	"prevent_xsrf": false,
	"anonymous_connection": null,
	"cors": [{
		"enabled": true,
		"allowMethods": [
           "GET",
           "POST",
           "HEAD",
           "PUT",
           "DELETE",
           "OPTIONS"
        ],
		"allowOrigin": ["*"],
		"maxAge": "3600"
    }],
	"cache_control": "no-cache, no-store",
	"default_file": "index.html"
}